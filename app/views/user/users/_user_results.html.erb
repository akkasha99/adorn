<% if users.size > 0 %>
    <% users.each do |user| %>
        <div class="w_100">
          <a href="javascript:;" onclick="get_partial('<%= user.id %>','/user/users/user_closet', 'true')">
            <div class="inner-h-con p0">
              <!--<img alt="" src="images/profile-pic.jpg">-->
              <% if user.avatar? && File.exists?(user.avatar.path(:thumb).to_s) %>
                  <%= image_tag user.avatar(:thumb) %>
              <% else %>
                  <%= image_tag "missing.png" %>
              <% end %>
              <div style="<%= "font-size: 10px!important;" if user.user_name.length > 25 %>" class="pro low_cas"><%= user.user_name %></div>
            </div>
          </a>
        </div>
    <% end %>
<% else %>
    <div class="w100">
      <a href="#">
        <div class="inner-h-con p0">
          <div class="pro">Nothing Found.</div>
        </div>
      </a>
    </div>
<% end %>