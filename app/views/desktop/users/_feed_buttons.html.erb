        <img src="/assets/adoorn-eye_active.png" alt=""> Adoorned</a>
  <% else %>
      <a href="javascript:;" onclick="feed_adoorn('<%= feed.class.name + feed.id.to_s %>','<%= feed.id %>','<%= feed.class.name %>',<%= current_user.id %>,'feed')"><img src="/assets/adoorn-eye.png" alt="">Adoorn</a>
  <% end %>
