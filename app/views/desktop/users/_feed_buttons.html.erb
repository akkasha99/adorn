    <div class="desk-black-built"></div>
  </div>
  <!--black-built end-->
</div>
<div class="desk-total-visitor"><span><%= number_to_human(feed.adorns.length) %></span>
  <img alt="" src="/assets/desk-eye.png"></div>


<script>
    function feed_adoorn(div_id, id, action, user_id, type) {
        if (action == "UserOutfit") {
            url = '/user/user_outfits/feed_adoorning?outfit_id=' + id + '&user_id=' + user_id + '&type=' + type
        }
        else {
            url = '/user/user_items/feed_adoorning?item_id=' + id + '&user_id=' + user_id + '&type=' + type
        }
        $.ajax({
            url: url,
            success: function (data) {
                if (data != 'error')
                    $('#' + div_id).html(data);
            },
            error: function (data) {

            }
        });
    }</script>